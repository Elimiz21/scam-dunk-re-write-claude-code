# ScamDunk History Database Configuration
# Copy this file to .env and fill in your values

# PostgreSQL Database URL
# Format: postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL="postgresql://postgres:password@localhost:5432/scamdunk_history"

# Path to main ScamDunk evaluation results
EVALUATION_RESULTS_PATH="../evaluation/results"

# Path to social media scan results
SOCIAL_MEDIA_RESULTS_PATH="../evaluation/reports"

# Dashboard server port
DASHBOARD_PORT=3001

# Optional: FMP API key for fetching additional price data
FMP_API_KEY=""
